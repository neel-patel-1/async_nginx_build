user qat qat;

load_module modules/ngx_ssl_engine_qat_module.so;
worker_processes 10;     
worker_cpu_affinity 0000000001111111111000000000011111111110;

ssl_engine{     
   use_engine qatengine;
   default_algorithms ALL;
   qat_engine{
      qat_notify_mode poll;
      qat_offload_mode async;
      qat_poll_mode heuristic;
}}
worker_rlimit_nofile 1000000;   

events  
{
  use epoll;    

  worker_connections 8192;  

  multi_accept on;  

  accept_mutex on;  
}

http    
{
          ssl_buffer_size 65536;    

          include       mime.types; 
          default_type  application/octet-stream;   
          sendfile on;  
          access_log off;   
        server 
        {
                listen       192.168.2.2:443 reuseport backlog=131072 so_keepalive=off ; 
                sendfile on;  

                tcp_nopush on;  

                tcp_nodelay on;  
                ssl_verify_client off;  
                ssl_session_tickets off;    
                access_log  off;  
                lingering_close off; 
                lingering_time 1;  
                server_name  server_1;  

                ssl                  on;    
                ssl_asynch           on;    
                ssl_certificate      server.crt;  
                ssl_certificate_key  server.key;  

                ssl_session_timeout  300s;  

                ssl_protocols  TLSv1.2; 

                ssl_ciphers ECDHE-RSA-AES128-GCM-SHA256:ECDHE-ECDSA-AES128-GCM-SHA256:AES128-CBC:AES128-GCM-SHA256;
                ssl_prefer_server_ciphers   on;  

                location /  
                {
                          index  index.html index.htm;
                }
        }
        server 
        {
